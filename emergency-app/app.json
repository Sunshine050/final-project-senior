{
  "expo": {
    "name": "Emergency SOS",
    "slug": "emergency-app",
    "scheme": "emergencyapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b0f19"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.emergency.sos",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "แอปต้องการใช้ตำแหน่งของคุณเพื่อส่งสัญญาณขอความช่วยเหลือและหาหน่วยแพทย์ใกล้เคียง"
      },
      "config": {
        "googleMapsApiKey": "YOUR_GOOGLE_MAPS_API_KEY"
      }
    },
    "android": {
      "package": "com.emergency.sos",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0b0f19"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CALL_PHONE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "isAndroidBackgroundLocationEnabled": false,
          "locationAlwaysAndWhenInUsePermission": "แอปต้องการตำแหน่งเพื่อช่วยค้นหาโรงพยาบาลและส่งข้อมูล SOS อย่างแม่นยำ",
          "locationWhenInUsePermission": "แอปต้องการตำแหน่งเพื่อช่วยค้นหาโรงพยาบาลและส่งข้อมูล SOS อย่างแม่นยำ"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiBaseUrl": "http://192.168.1.3:3000",
      "longdoMapKey": "bc7aa650e38fc00dc32b5902a0f07757",
      "router": {
        "origin": false
      }
    }
  }
}
